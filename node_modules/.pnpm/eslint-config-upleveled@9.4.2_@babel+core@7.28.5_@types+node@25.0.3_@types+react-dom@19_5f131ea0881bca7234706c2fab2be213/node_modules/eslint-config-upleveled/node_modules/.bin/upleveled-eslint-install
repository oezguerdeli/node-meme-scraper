#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/oezgu/Projekte/01_Meme-Scraper/node-meme-scraper/node_modules/.pnpm/eslint-config-upleveled@9.4.2_@babel+core@7.28.5_@types+node@25.0.3_@types+react-dom@19_5f131ea0881bca7234706c2fab2be213/node_modules/eslint-config-upleveled/bin/node_modules:/Users/oezgu/Projekte/01_Meme-Scraper/node-meme-scraper/node_modules/.pnpm/eslint-config-upleveled@9.4.2_@babel+core@7.28.5_@types+node@25.0.3_@types+react-dom@19_5f131ea0881bca7234706c2fab2be213/node_modules/eslint-config-upleveled/node_modules:/Users/oezgu/Projekte/01_Meme-Scraper/node-meme-scraper/node_modules/.pnpm/eslint-config-upleveled@9.4.2_@babel+core@7.28.5_@types+node@25.0.3_@types+react-dom@19_5f131ea0881bca7234706c2fab2be213/node_modules:/Users/oezgu/Projekte/01_Meme-Scraper/node-meme-scraper/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/oezgu/Projekte/01_Meme-Scraper/node-meme-scraper/node_modules/.pnpm/eslint-config-upleveled@9.4.2_@babel+core@7.28.5_@types+node@25.0.3_@types+react-dom@19_5f131ea0881bca7234706c2fab2be213/node_modules/eslint-config-upleveled/bin/node_modules:/Users/oezgu/Projekte/01_Meme-Scraper/node-meme-scraper/node_modules/.pnpm/eslint-config-upleveled@9.4.2_@babel+core@7.28.5_@types+node@25.0.3_@types+react-dom@19_5f131ea0881bca7234706c2fab2be213/node_modules/eslint-config-upleveled/node_modules:/Users/oezgu/Projekte/01_Meme-Scraper/node-meme-scraper/node_modules/.pnpm/eslint-config-upleveled@9.4.2_@babel+core@7.28.5_@types+node@25.0.3_@types+react-dom@19_5f131ea0881bca7234706c2fab2be213/node_modules:/Users/oezgu/Projekte/01_Meme-Scraper/node-meme-scraper/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/install.js" "$@"
else
  exec node  "$basedir/../../bin/install.js" "$@"
fi
